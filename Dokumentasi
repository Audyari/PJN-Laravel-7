1. composer create-project --prefer-dist laravel/laravel:^7.0 blog
    
    composer install
    php artisan key:generate
    composer dump-autoload
    php artisan cache:clear
    php artisan config:cache


2. php artisan serve

3. php -S localhost:8000

4. php artisan make:test ContohTest

5. php artisan make:test ContohTest --unit

6. php artisan test

7.  // untuk menyimpan perubahan yang sudah diperbarui
    php artisan config:cache

    // untuk menghapus cache
    php artisan config:clear

8. // cara registrasi Service Provider
    php artisan make:provider FooBarServiceProvider

9. // untuk menghapus file compiled
    php artisan clear-compiled

10. // untuk melihat daftar route
    php artisan Route:list

11. // untuk memasukkan view ke cache
    php artisan view:cache

12. // untuk menghapus cache view
    php artisan view:clear

13. // untuk menjalankan aplikasi di mode production
    npm install
    npm run dev
    npm run prod

14. // pembuatan controller
    php artisan make:controller ContohController
